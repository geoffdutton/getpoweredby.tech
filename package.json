{
  "name": "getpoweredby.tech",
  "version": "0.2.0",
  "author": "Geoff Dutton <g.dutton@gmail.com>",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "NODE_ENV=production vue-cli-service build",
    "test": "vue-cli-service test:unit --coverage",
    "tdd": "vue-cli-service test:unit --watch",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": "git@github.com:geoffdutton/getpoweredby.tech.git",
  "dependencies": {
    "@mapbox/polyline": "^1.1.1",
    "axios": "^0.27.2",
    "buefy": "^0.9.21",
    "d3": "5",
    "vue": "2",
    "vue-router": "3",
    "vuex": "3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3",
    "@vue/cli-plugin-unit-jest": "3",
    "@vue/cli-service": "3",
    "@vue/test-utils": "^1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^28.1.3",
    "coveralls": "^3.1.1",
    "jest-localstorage-mock": "^2.4.21",
    "standard": "^17.0.0",
    "vue-template-compiler": "^2.7.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "standard": {
    "env": [
      "jasmine"
    ],
    "globals": [
      "jest",
      "test"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "modulePaths": [
      "<rootDir>/node_modules",
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  }
}
